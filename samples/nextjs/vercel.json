{
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "name": "jss",
    "version": 2,
    "github": {
      "autoJobCancelation": false,
      "enabled": true
    },
    "routes": [
        {
          "src": "/favicon.ico",
          "headers": { "Cache-Control": "public, max-age=3600" },
          "continue": true
        },
        {
          "src": "/assets/(.*)",
          "headers": { "Cache-Control": "public, max-age=31556952, immutable" },
          "continue": true
        }
    ]
}